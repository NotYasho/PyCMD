[
	{
		"name": "create",
		"description": "Create a new Project + Repo",
		"usage": "create [project_name].[language-ext] [flags]",
		"parameters": [
			"py",
			"js",
			"web/html",
			"java"
		],
		"example": "pycmd create google.html -p",
		"flags": "-p -private, -l  -local -y"
	},
	{
		"name": "ls",
		"description": "List all projects",
		"usage": "ls [language]",
		"parameters": [
			"py",
			"js",
			"web/html",
			"java",
			"repos"
		],
		"example": "pycmd ls py"
	},
	{
		"name": "help",
		"description": "Display help for a command",
		"usage": "help [command]",
		"example": "pycmd help ls"
	},
	{
		"name": "version",
		"description": "Display version information",
		"usage": "pycmd version"
	},
	{
		"name": "rm",
		"description": "Remove a project",
		"parameters": [
			"py",
			"js",
			"web/html",
			"java",
			"repos"
		],
		"usage": "rm [project_name]",
		"example": "pycmd rm minecraft.java"
	},
	{
		"name": "open",
		"description": "Open a project in the default editor",
		"parameters": [
			"py",
			"js",
			"web/html",
			"java"
		],
		"usage": "open [project_name]",
		"example": "pycmd open hack_elon_musk.py"
	},
	{
		"name": "setup",
		"description": "Configure PYCMD, your preferences",
		"parameters": ["git"],
		"usage": "setup",
		"example": "pycmd setup git"
	}
]
